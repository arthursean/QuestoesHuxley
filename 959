#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int separador, numero, cont, cont2 = 1, x=1;
    scanf("%d%d%d", &separador, &numero, &cont);
    for(int i = 1; i<=numero; i+=cont)
    {
        if(separador*cont2 == x)
        {
            printf("%d\n", i);
            x+=1;
            cont2+=1;
        }
        else
        {
            printf("%d ", i);
            x+=1;
        }
    }
    
	return 0;
}
